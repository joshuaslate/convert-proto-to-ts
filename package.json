    "prettier": "^3.0.0",
    "rollup": "^3.26.3",
    "tslib": "^2.6.1",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "find-up": "^6.3.0",
    "inquirer": "^9.2.8",
    "protobufjs": "^7.2.4",
    "ts-node": "^10.9.1",
